<div class='row'>
  <div class='col-xs-6'>
    <%= simple_form_for  [:manage, @panel, @slide ] do |f|  %>
      <%= f.association :entry, :collection => @entries_collection %>
      <%= f.input :starts_at %>
      <%= f.input :ends_at %>
      <%= f.input :duration %>
      <%= f.button :submit  %>
    <% end %>
  </div>  
</div>
